# General 
Host *
    AddKeysToAgent yes
    UseKeychain yes

# GitHub default conf
Host github.com*
     HostName github.com
     User git

# GitHub own account
Host github.com
    IdentityFile ~/.ssh/github-hamsternik

# Bitbucket own account
Host bitbucket.org
     User git
     HostName bitbucket.org
     IdentityFile ~/.ssh/github-hamsternik

# GitLab Nuvo Work Account 
Host gitlab.dev.nvsrc.com
    User git
    HostName gitlab.dev.nvsrc.com
    IdentityFile ~/.ssh/gitlab-nuvo

