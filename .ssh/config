# --- General Settings ---
Host *
    AddKeysToAgent yes
    UseKeychain yes
# --- GitHub Default Configuration ---
Host github.com*
    User git
    HostName github.com
    IdentityFile ~/.ssh/github-hamsternik
# --- Bitbucket own account ---
Host bitbucket.org
    User git
    HostName bitbucket.org
    IdentityFile ~/.ssh/github-hamsternik
